    <!-- Static navbar -->
    <div class="navbar navbar-default navbar-static-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">Aling Nena's</a>
        </div>
        <div class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li><a href="/">Home</a></li>
            <li><a href="/about">About</a></li>
            <li class="active"><a href="/buy">Shop</a></li>
        
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>

<img class="ri" src="../banners/BannerPurple.jpg" />

<div class="content">
<h2>Congratulations!</h2>
<br />
<h4>You gave:&nbsp<%= @given %></h4>
<br />
<h4>Your due:&nbsp<%= @due %></h4>
<br />
<h4>You bought:&nbsp<%= @quantity %>&nbsp<%= @title %></h4>
<br />
<h4>Your change is <%= @change %></h4>
<br />
<h4>You will receive your change in the form of:</h4>
<ul>
<% @hash.each_pair do |key, value| %>
<li><%= value %>&nbsp<%= key %></li>
<% end %>
</ul>
<a href = "/"><input type="button" value="Done!"></a>
</div>
